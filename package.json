{
  "name": "postcss-calc",
  "version": "6.0.2",
  "description": "PostCSS plugin to reduce calc()",
  "keywords": [
    "css",
    "postcss",
    "postcss-plugin",
    "calculation",
    "calc"
  ],
  "main": "dist/index.js",
  "files": [
    "dist",
    "LICENSE"
  ],
  "scripts": {
    "prepublish": "npm run build && del-cli dist/__tests__",
    "build": "del-cli dist && cross-env BABEL_ENV=publish babel src --out-dir dist",
    "pretest": "eslint src && npm run build",
    "test": "ava src/__tests__/"
  },
  "author": "Maxime Thirouin",
  "license": "MIT",
  "repository": "https://github.com/postcss/postcss-calc.git",
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "eslint-config-i-am-meticulous"
  },
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.4.0",
    "babel-register": "^6.18.0",
    "cross-env": "^4.0.0",
    "del-cli": "^0.2.1",
    "eslint": "^3.12.2",
    "eslint-config-i-am-meticulous": "^6.0.1",
    "eslint-plugin-babel": "^4.0.0",
    "eslint-plugin-import": "^2.2.0"
  },
  "dependencies": {
    "css-unit-converter": "^1.1.1",
    "postcss": "^7.0.2",
    "postcss-selector-parser": "^5.0.0-rc.3",
    "reduce-css-calc": "^2.0.0"
  },
  "ava": {
    "require": "babel-register"
  }
}
